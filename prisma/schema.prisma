// prisma/schema.prisma
datasource db {
  provider = "sqlite"
 url      = env("DATABASE_URL")  // Archivo SQLite local
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id       Int     @id @default(autoincrement())
  email String @unique
  username String  @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  carrera String 
}
